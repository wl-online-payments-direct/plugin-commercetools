{
  "name": "@worldline/ctintegration-app",
  "version": "1.0.0",
  "description": "Module containing App logic",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w -preserveWatchOutput",
    "lint": "eslint src/**/*.ts",
    "lint-fix": "eslint --fix src/**/*.ts",
    "pretty": "prettier --write 'src/**/*.ts'",
    "prepare": "cd .. && husky install app/.husky",
    "precommit": "npm run lint-fix && npm run pretty",
    "prepush": "npm run lint"
  },
  "files": [
    "/dist"
  ],
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.8.7",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@worldline/ctintegration-ct": "^1.0.0",
    "@worldline/ctintegration-db": "^1.0.0",
    "@worldline/ctintegration-psp": "^1.0.0",
    "@worldline/ctintegration-util": "^1.0.0"
  }
}
