{
  "name": "@worldline/ctintegration-web",
  "version": "1.0.0",
  "description": "Package for worldline",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "node ./dist/index.js",
    "dev": "nodemon src/index.ts",
    "eslint": "eslint . --ext .ts",
    "start:debug": "nodemon --exec \"node --inspect-brk=0.0.0.0:9229 --require ts-node/register src/index.ts\"",
    "lint": "eslint src/**/*.ts",
    "lint-fix": "eslint --fix src/**/*.ts",
    "pretty": "prettier --write 'src/**/*.ts'",
    "prepare": "cd .. && husky install web/.husky",
    "precommit": "npm run lint-fix && npm run pretty",
    "prepush": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "license": "ISC",
  "homepage": "",
  "dependencies": {
    "@types/node": "^18.11.18",
    "@worldline/ctintegration-app": "^1.0.0",
    "@worldline/ctintegration-util": "^1.0.0",
    "dotenv": "^16.0.3",
    "http-status-codes": "^2.2.0",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.1"
  },
  "lint-staged": {
    "src/**/*.{ts,js}": [
      "eslint",
      "prettier --write"
    ]
  },
  "nodemonConfig": {
    "verbose": false,
    "execMap": {
      "ts": "ts-node",
      "js": "node"
    },
    "watch": [
      "../app/dist/",
      "../ct/dist/",
      "../db/dist/",
      "../psp/dist/",
      "../util/dist/"
    ],
    "ignore": [
      ".git",
      "node_modules/**/node_modules",
      "*.test.ts"
    ],
    "ext": "ts,js"
  }
}
